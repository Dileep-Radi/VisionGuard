<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.visionguard">

    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:label="Vision Guard"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">

        <activity android:name=".SafeBrowserActivity"/>
        <activity android:name=".StartSafeActivity"/>
        <activity android:name=".ReportsActivity"/>
        <activity android:name=".SettingsActivity"/>
        <activity android:name=".EyeGameActivity"/>
        <activity android:name=".StarCatchActivity"/>
        <activity android:name=".BlinkChallengeActivity"/>
        <activity android:name=".TwentyRuleActivity"/>

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

    </application>
</manifest>
